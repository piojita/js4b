/* ========== *
* Challenge 17 *
* =========== */
	console.log('--------Challenge 17');
// Utilize the two constants below and have them match the statements within our control flow. Get each condition within our control flow statement to be matched. Run the code each time you change these variable delcarations.

const mood = 'happy';
const weather = 'rain';

 if (mood === 'happy') {
  if (weather === 'rain') {
    console.log('you like when it rains');
  } else if (weather === 'snow') {
    console.log('you like when it snows');
  } else if (weather === 'sun') {
    console.log('you like when its sunny');
  } else {
    console.log('enter a weather condition our control flow accounts for while you feel happy');
  }
} else if (mood === 'sad') {
  if (weather === 'rain') {
    console.log('the rain makes you sad');
  } else if (weather === 'snow') {
    console.log('the snow makes you sad');
  } else if (weather === 'sun') {
    console.log('the sun makes you sad');
  } else {
    console.log('enter a weather condition our control flow accounts for while you feel sad');
  }
} else {
  console.log('enter a mood that our control flow accounts for');
}

// After getting each if/else block to successfully log to the console comment out the code above.



