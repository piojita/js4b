	console.log('--------Challenge 16');
// Declare two variables and set them equal to the right specifications to make all three instances of the control flow hit.

  let country = 'USA';
  let age = 19;
  if (age >= 18 && country === 'London') {
    console.log('you can legally drink alcohol in London')
  } else if (age <= 21 && country === 'USA') {
    console.log('you cannot legally drink alcohol in // America')
  } else {
   console.log('age and country are not specified for')
  }

