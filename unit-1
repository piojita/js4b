// Create an object with three key/value pairs where are keys are: name, age, hasDog.

  let myObject = { 
    name: 'vivi', 
    age:32, 
    hasDog: true,
  };
  console.log(myObject);


/* ========== *
* Challenge 12 *
* =========== */

// Individually print each of the values in your object to the console using dot and bracket notation.
  console.log(myObject.name);
  console.log(myObject['name']);
  console.log(myObject.age);
  console.log(myObject['age']);
  console.log(myObject.hasDog);
  console.log(myObject['hasDog']);

// After successfully printing each value with both bracket and dot notation to the console comment out the console logs above.


